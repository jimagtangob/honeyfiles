<div class="content-header">
  
  <div class="title">
    <i class="icon-tasks"></i>
    Campaigns

  </div>

  <div class="content-options">
    <a class="button create-new-campaign" href="#">
      <i class="icon-plus"></i>
      New Campaign
    </a>
  </div>

</div>


<div class="data">

  {{#campaigns}}

  <div class="campaign" data-id="{{id}}" data-enabled="{{enabled}}">
    <div class="name">{{name}}</div>
    <div class="timestamp">{{createdAt}}</div>
  </div>

  {{/campaigns}}

</div>
